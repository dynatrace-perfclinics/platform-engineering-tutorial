{
    "displayName": "{{.name}}",
    "enabled": true,
    "nameFormat": "platform | {ProcessGroup:DetectedName}",
    "rules": [
      {
        "comparisonInfo": {
          "caseSensitive": true,
          "negate": false,
          "operator": "EQUALS",
          "type": "STRING",
          "value": "{{ .string_match }}"
        },
        "key": {
          "attribute": "CLOUD_APPLICATION_NAMESPACE_LABELS",
          "dynamicKey": "dt.owner",
          "type": "STRING"
        }
      }
    ],
    "type": "PROCESS_GROUP"
  }